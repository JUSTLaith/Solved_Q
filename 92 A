#include <iostream>
#include <vector>
#include <algorithm>
#include <cstdlib>
#include <string>
#include <cctype>
#include <cstdio>
#include <cmath>
#include <queue> 
using namespace std;


int main()
{
	int n,m;
	cin>>n>>m;
	for(int i=1;m>=i;i++)
	{
		if(i<=n)
		m-=i;
		if(i==n)
			i=0;
	}
	cout<<m<<endl;
	return 0;
}
