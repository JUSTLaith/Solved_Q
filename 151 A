#include <iostream>
#include <vector>
#include <algorithm>
#include <cstdlib>
#include <string>
#include <cctype>
#include <cstdio>
#include <cmath>
#include <queue> 
using namespace std;


int main()
{
	int n,l,k,c,d,p,nl,np,z[3],min;
	cin>>n>>l>>k>>c>>d>>p>>nl>>np;
	z[0]=d*c/n;
	z[1]=(k*l)/(nl*n);
	z[2]=p/(np*n);
	min=z[0];
	if(min>z[1])
		min=z[1];
	if(min>z[2])
		min=z[2];
	cout<<min<<endl;



	return 0;
}
